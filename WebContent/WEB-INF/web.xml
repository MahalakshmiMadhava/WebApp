<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>TP</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
 <display-name>Sample Project</display-name>
  <listener>
  	<listener-class>web.MyListener</listener-class>
  </listener>
  <servlet>
  	<servlet-name>LoginCont</servlet-name>
  	<servlet-class>web.LoginCont</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Pdf</servlet-name>
  	<servlet-class>web.Pdf</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ThingsController</servlet-name>
  	<servlet-class>web.ThingsController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UploadServ</servlet-name>
  	<servlet-class>web.UploadServ</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginCont</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginCont</servlet-name>
  	<url-pattern>/reg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginCont</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Pdf</servlet-name>
  	<url-pattern>/vvv</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ThingsController</servlet-name>
  	<url-pattern>/SL</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ThingsController</servlet-name>
  	<url-pattern>/SL1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ThingsController</servlet-name>
  	<url-pattern>/SL2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ThingsController</servlet-name>
  	<url-pattern>/newword</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ThingsController</servlet-name>
  	<url-pattern>/updateword</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ThingsController</servlet-name>
  	<url-pattern>/updatedesc</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ThingsController</servlet-name>
  	<url-pattern>/removeword</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ThingsController</servlet-name>
  	<url-pattern>/pdf</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UploadServ</servlet-name>
  	<url-pattern>/upload</url-pattern>
  </servlet-mapping>
 <error-page>
    <error-code>500</error-code>
    <location>/My.jsp</location>
  </error-page>
</web-app>